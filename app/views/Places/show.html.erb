<h1><%= @place.name %></h1>

    <% for post in @place.posts %>
        <h2><%= "#{post.title}" %></h2>
        <h3><%= "#{post.posted_on}" %></h3>
        <p><%= "#{post.description}" %><p>
    <% end %>

<p>
    <a href="/posts/new?palce_id=<%= @place.id %>">New Post</a>
</p>

<p>
    <a href="/places">Back to Places</a>
</p>